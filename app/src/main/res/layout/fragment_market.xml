<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical" >


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="62dp"
        android:layout_height="48dp"
        android:layout_marginLeft="70dp"
        android:src="@drawable/dia100"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/diamond_100"
        app:layout_constraintHorizontal_bias="0.918"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/diamond_100"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="61dp"
        android:layout_height="48dp"
        android:layout_marginLeft="70dp"
        android:src="@drawable/dia50"
        app:layout_constraintBottom_toTopOf="@+id/imageView5"
        app:layout_constraintEnd_toStartOf="@+id/diamond_50"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/diamond_50"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="64dp"
        android:layout_height="48dp"
        android:layout_marginLeft="70dp"
        android:src="@drawable/dia10"
        app:layout_constraintBottom_toTopOf="@+id/diamond_50"
        app:layout_constraintEnd_toStartOf="@+id/diamond_10"
        app:layout_constraintHorizontal_bias="0.983"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/diamond_10"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="172dp"
        android:layout_marginRight="40dp"
        android:background="@drawable/radian_txtview"
        android:gravity="center"
        android:text="매칭을 위해 다이아를               구매해주세요!"
        android:fontFamily="@font/poppinsmedium"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/diamond_10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.062" />

    <Button
        android:id="@+id/diamond_10"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="120dp"
        android:layout_marginRight="70dp"
        android:layout_marginBottom="44dp"
        android:background="@drawable/radian_btn2"
        android:text="다이아몬드 10개"
        android:fontFamily="@font/poppinsmedium"
        app:layout_constraintBottom_toTopOf="@+id/diamond_50"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/diamond_50"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="120dp"
        android:layout_marginRight="70dp"
        android:layout_marginBottom="44dp"
        android:background="@drawable/radian_btn2"
        android:text="다이아몬드 50개"
        android:fontFamily="@font/poppinsmedium"
        app:layout_constraintBottom_toTopOf="@+id/diamond_100"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.7"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/diamond_100"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="120dp"
        android:layout_marginRight="70dp"
        android:layout_marginBottom="144dp"
        android:background="@drawable/radian_btn2"
        android:text="다이아몬드 100개"
        android:fontFamily="@font/poppinsmedium"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="126dp"
        android:layout_height="110dp"
        android:src="@drawable/ic_cash"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>